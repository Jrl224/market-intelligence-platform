# Search & Trends
SERPAPI_KEY=your_serpapi_key  # Required for Google Trends data

# News Sources
NEWSAPI_KEY=your_newsapi_key  # Primary news source
BING_NEWS_KEY=your_bing_key  # Optional backup news source

# Social Media Analysis
REDDIT_CLIENT_ID=your_reddit_client_id  # For Reddit sentiment
REDDIT_CLIENT_SECRET=your_reddit_client_secret
YOUTUBE_API_KEY=your_youtube_key  # For YouTube comments

# Academic Research
SEMANTIC_SCHOLAR_KEY=your_semantic_scholar_key  # 200M+ papers
CORE_API_KEY=your_core_key  # Optional - Open access papers
CROSSREF_EMAIL=your_email@example.com  # Required for Crossref

# Economic Data
FRED_API_KEY=your_fred_key  # Federal Reserve Economic Data
CENSUS_API_KEY=your_census_key  # Optional US Census data

# Financial Markets
ALPHA_VANTAGE_KEY=your_alpha_vantage_key  # Stock market data
RAPIDAPI_KEY=your_rapidapi_key  # Optional for Yahoo Finance

# AI Providers (at least one required)
OPENAI_API_KEY=your_openai_key  # GPT-4 for insights
ANTHROPIC_API_KEY=your_anthropic_key  # Claude for analysis
GEMINI_API_KEY=your_gemini_key  # Google's AI model

# Additional Services (Optional)
HUGGINGFACE_TOKEN=your_huggingface_token  # For ML models
FIRECRAWL_API_KEY=your_firecrawl_key  # Web scraping

# Note: The platform works with partial API keys.
# Each missing API will disable its specific features only.
# Minimum recommended: SERPAPI_KEY + NEWSAPI_KEY + one AI provider
